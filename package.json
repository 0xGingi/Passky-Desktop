{
  "name": "passky-desktop",
  "version": "0.0.1",
  "author": "Rabbit Company",
  "description": "Desktop application for Passky (password manager)",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.7"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "passky_desktop",
            "description": "Desktop application for Passky (password manager)",
            "iconUrl": "https://raw.githubusercontent.com/Rabbit-Company/Passky-Desktop/main/images/logo.ico",
            "setupIcon": "https://raw.githubusercontent.com/Rabbit-Company/Passky-Desktop/main/images/logo.ico"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "Rabbit Company",
              "homepage": "https://rabbit-company.com",
              "description": "Desktop application for Passky (password manager)"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "maintainer": "Rabbit Company",
              "homepage": "https://rabbit-company.com",
              "description": "Desktop application for Passky (password manager)"
            }
          }
        }
      ]
    }
  }
}
